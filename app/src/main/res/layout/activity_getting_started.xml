<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grade"
    tools:context=".GettingStartedActivity">


    <TextView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/H_aod"
        android:layout_marginTop="30dp"
        android:text="Get Started"
        android:textSize="25dp" />


    <Button
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="21dp"
        android:layout_marginEnd="21dp"
        android:text="Logout" />


    <TextView
        android:id="@+id/H_aod"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="143dp"
        android:text="@string/category_1"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/chll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/H_aod"
        android:layout_marginTop="187dp"
        android:text="@string/category_2"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/disc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/H_aod"
        android:layout_marginTop="234dp"
        android:text="@string/category_3"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/resu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/H_aod"
        android:layout_marginBottom="255dp"
        android:text="@string/category_4"
        android:textSize="24sp" />
</RelativeLayout>