<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="1px"
                android:color="#00ffff"/>

            <solid
                android:color="#6DFFFF"/>

            <padding
                android:left="4dp"
                android:right="4dp"
                android:top="4dp"
                android:bottom="4dp"/>

            <corners
                android:radius="48dp"/>
        </shape>
    </item>

    <item>
        <shape>
            <stroke
                android:width="1px"
                android:color="#00ffff" />

            <solid
                android:color="#dcffff" />

            <padding
                android:left="4dp"
                android:right="4dp"
                android:top="4dp"
                android:bottom="4dp"/>

            <corners
                android:radius="48dp" />
        </shape>
    </item>
</selector>